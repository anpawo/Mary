import list

function main() -> int {
    // arithmetic
    print(3 + 2);
    print(3 - 2);
    print(3 * 2);
    print(3 / 2);

    // cmp
    print(is(1, "int"));
    print(1 == 1);
    print(1 < 2);
    
    // io
    print("stdout");
    print(getline());
    eprint("stderr");
    
    // struct
    a: list = elem{data = 1, next = empty {}};
    print(a.data);
    print(set(a, "data", 5));

    // array and string
    print(length(int[1, 2, 3]));
    print(length("hello"));
    print(insert("hello", 5, '!'));
    print(insert(int[1, 2], 2, 3));
    print(append("hello", '!'));
    print(append(int[1, 2], 3));
    print(at("hello", 2));
    print(at(int[1, 2, 3], 1));

    // conversion
    print(toInt(1));
    print(toInt("1"));
    print(toInt(1.0));
    print(toInt(true));
    print(toInt('A'));

    print(toFloat(1));
    print(toFloat("1.0"));
    print(toFloat(1.0));
    print(toFloat(true));

    print(toChar(65));
    print(toChar('A'));

    print(toString("hello"));
    print(toString('A'));
    print(toString(1));
    print(toString(1.0));
    print(toString(true));
    print(toString(int[1, 2]));
    print(toString(empty{}));
    print(toString(NULL));

    exit(1);
}
